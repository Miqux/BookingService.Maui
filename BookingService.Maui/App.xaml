<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:BookingService.Maui"
             xmlns:converter="clr-namespace:BookingService.Maui.Converter"
             x:Class="BookingService.Maui.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <converter:NegateBooleanConverter x:Key="NegateBooleanConverter" />
            <converter:DescriptionEnumConverter x:Key="DescriptionEnumConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
